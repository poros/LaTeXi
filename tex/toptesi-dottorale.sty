      \edef\@submitdate{\noexpand#1
      \noexpand\oldstylenums{\1} -- \noexpand\oldstylenums{\2}}%
    \fi
      \let\1\undefined
      \let\2\undefined
  \else
    \def\@submitdate{#1 #2}%
  \fi
}
